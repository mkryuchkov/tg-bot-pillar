{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "BotConfiguration": {
    "Token": "<Token>",
    "Host": "<Host>",
    "CertPath": "<CertPath>"
  },
  "Kestrel": {
    "Endpoints": {
      "HTTPS": {
        "Url": "https://+:8443",
        "Certificate": {
          "Path": "<Kestrel:Endpoints:HTTPS:Certificate:Path>",
          "Password": "<Kestrel:Endpoints:HTTPS:Certificate:Password>"
        }
      }
    }
  }
}